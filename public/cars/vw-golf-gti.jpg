<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600">
  <!-- Background -->
  <rect width="800" height="600" fill="#e0e7ff"/>
  
  <!-- Car body -->
  <rect x="200" y="250" width="400" height="150" rx="20" fill="#ef4444"/>
  
  <!-- Car roof -->
  <rect x="300" y="200" width="200" height="100" rx="15" fill="#ef4444"/>
  
  <!-- Windows -->
  <rect x="320" y="220" width="60" height="60" fill="#bae6fd"/>
  <rect x="420" y="220" width="60" height="60" fill="#bae6fd"/>
  
  <!-- Wheels -->
  <circle cx="280" cy="400" r="40" fill="#1f2937"/>
  <circle cx="280" cy="400" r="25" fill="#6b7280"/>
  <circle cx="520" cy="400" r="40" fill="#1f2937"/>
  <circle cx="520" cy="400" r="25" fill="#6b7280"/>
  
  <!-- Headlights -->
  <circle cx="190" cy="300" r="15" fill="#fef08a"/>
  <circle cx="610" cy="300" r="15" fill="#fef08a"/>
  
  <!-- Details -->
  <rect x="250" y="320" width="300" height="10" fill="#fbbf24"/>
  <text x="400" y="370" font-family="Arial" font-size="24" fill="white" text-anchor="middle">VW Golf GTI</text>
</svg>